net: "train_test_lstm.prototxt"
#test_iter: 100
#test_state: { stage: 'test-on-test' }
#test_interval: 1000
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 20000
display: 40
max_iter: 30000
momentum: 0.9
weight_decay: 0.005
snapshot: 2500
snapshot_prefix: "models/"
solver_mode: GPU
random_seed: 1701
average_loss: 1000
clip_gradients: 5
